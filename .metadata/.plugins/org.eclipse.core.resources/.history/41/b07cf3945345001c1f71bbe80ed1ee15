<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>ajax test to vanila js</title>
</head>
<script>
	function ajax() {
		var xhr = new XMLHttpRequest();

		xhr.onreadystatechange = function() {
			if (xhr.readyState === 4) {
				if (xhr.status === 200) {
					console.log("작업내용 작성");
				}
			}
		};

		xhr.open("POST", "/todo/TodoTypeServlet", true);
		xhr.setRequestHeader("Content-Type",
				"application/x-www-form-urlencoded");
		xhr.send(null);
	}
</script>
<body>
	<h3>hi</h3>
</body>
</html>